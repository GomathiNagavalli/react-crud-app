{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/users/:match*",
            "destination": "/api/users.js"
        },
        {
            "source": "/api/users",
            "destination": "/api/users.js"
        },
        {
            "source": "/(.*)",
            "destination": "/client/index.html"
        }
    ]
}